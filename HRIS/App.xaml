<Application x:Class="HRIS.Controller.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HRIS.Controller"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:StaffController x:Key="staffController" />
        <ObjectDataProvider x:Key="x_StaffList"
                            ObjectInstance="{StaticResource staffController}"
                            MethodName="GetStaffViewList">
        </ObjectDataProvider>
        <ObjectDataProvider x:Key="x_Category"
                            ObjectType="{x:Type sys:Enum}"
                            MethodName=""
                            >
            
        </ObjectDataProvider>
    </Application.Resources>
</Application>
